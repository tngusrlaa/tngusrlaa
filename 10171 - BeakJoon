/**********************/
/*    Print a cat     */
/*      \    /\       */
/*       )  ( ')      */
/*       (  /  )      */
/*        \(__)|      */
/**********************/

#include <stdio.h>

int main()
{
    //If you want to enter '," or \, put in "\" befor it.
    printf("\\    /\\"); 
    printf("\n");
    printf(" )  ( ')");
    printf("\n");
    printf("(  /  )");
    printf("\n");
    printf(" \\(__)\|");
}
