/************************/
/*    Print a dog       */
/*      |\\_/|          */
/*      |q p|   /}      */
/*      ( 0 )"""\       */
/*      |"^"`    |      */
/*      ||_/=\\__|      */
/**********************/

#include <stdio.h>

int main()
{
    //If you want to enter '," or \, put in "\" befor it.
    printf("|\\_/|");
    printf("\n");
    printf("|q p|   /}");
    printf("\n");
    printf("( 0 )\"\"\"\\");
    printf("\n");
    printf("|\"^\"`    |");
    printf("\n");
    printf("||_/=\\\\__|");
}
